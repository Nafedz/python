<script>
    $(function() {
        $("#poro-facet-tabs").tabs();
        $("#poro-search-facets").show();
    });
    function remove_selected_facet(deselect_url) {
        var selected = $("#poro-facet-tabs").tabs('option', 'active');
        var selectedTabTitle = $($("#poro-facet-tabs li")[selected]).text();
        window.location = deselect_url + '#' + selectedTabTitle.trim();
    }
