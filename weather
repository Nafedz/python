ndValue(a){var b=undefined;if(jQuery.isNumeric(a)&&(a>=0)&&(a<=360)){if(a>22.5&&a<67.5){b="koillistuulta"}else{if(a>=67.5&&a<=112.5){b="itätuulta"}else{if(a>112.5&&a<157.5){b="kaakkoistuulta"}else{if(a>=157.5&&a<=202.5){b="etelätuulta"}else{if(a>202.5&&a<247.5){b="lounaistuulta"}else{if(a>=247.5&&a<=292.5){b="länsituulta"}else{if(a>292.5&&a<337.5){b="luoteistuulta"}else{b="pohjoistuulta"}}}}}}}if(a==0){b="tyyntä"}else{b+="\xa0("+Math.round(a)+"\xb0)"}}return b}function isInt(b){if(isNaN(b)){return false}var a=parseFloat(b);return(a|0)===a}function formatTotalCloudCover(a){var c=undefined;if(isInt(a)){var b=parseInt(a);switch(b){case 0:c="selkeää\xa0(0/8)";break;case 1:c="melkein selkeää\xa0(1/8)";break;case 2:c="melkein selkeää\xa0(2/8)";break;case 3:c="puolipilvistä\xa0(3/8)";break;case 4:c="puolipilvistä\xa0(4/8)";break;case 5:c="puolipilvistä\xa0(5/8)";break;case 6:c="melkein pilvistä\xa0(6/8)";break;case 7:c="melkein pilvistä\xa0(7/8)";break;case 8:c="pilvistä\xa0(8/8)";break;case 9:c="ei voitu määrittää\xa0(9/8)";break}}return c}function formatVisibility(a){var b=undefined;if(jQuery.isNumeric(a)&&(a>=0)){if(a<1000){b=Math.round(a)+"\xa0m"}else{if(a<10000){b=(0.1*Math.round(0.01*a)).toFixed(1)+"\xa0km"}else{b=Math.round(0.001*a)+"\xa0km"}}}return b}function getXAxisSlideDelta(b){var a;if(b.tickInterval>7200000){a=10*24*60*60*1000}else{a=8*60*60*1000}return a}function slideXAxisBackwards(b){var a=0;if(b.min>b.dataMin){a=getXAxisSlideDelta(b)}b.setExtremes(b.min-a,b.max-a)}function slideXAxisForwards(b){var a=0;if(b.max<b.dataMax){a=getXAxisSlideDelta(b)}b.setExtremes(b.min+a,b.max+a)}function renderTimeNavigatorBackground(g,i,h,b,k){var f=g.renderer;var c="custom-time-navigator";var e=0;var j=4;var d="#d7d7d7";var a="#f7f7f7";f.rect(i,h,b,k,e).attr({"stroke-width":1,stroke:d,fill:a,zIndex:0,"class":c}).add();f.rect(i,h,b,j,e).attr({"stroke-width":0,stroke:d,fill:d,zIndex:4,"class":c}).add()}function renderTimeNavigatorButtons(i,c,k,j,g,d,m){var h=i.renderer;var b