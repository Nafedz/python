,"short");jQuery("div.local-weather-forecast-metadata > span.probabilistic").css("display","none")}function handle_forecast_mid_click(){jQuery("li.local-weather-forecast-menu-item.selected").removeClass("selected");jQuery("li.local-weather-forecast-menu-item.mid").addClass("selected");jQuery("div.local-weather-forecast.selected").removeClass("selected");jQuery("div.local-weather-forecast.mid").addClass("selected");jQuery('input[name="forecast"]').attr("value","mid");jQuery("div.local-weather-forecast-metadata > span.probabilistic").css("display","none")}function handle_forecast_long_click(){jQuery("li.local-weather-forecast-menu-item.selected").removeClass("selected");jQuery("li.local-weather-forecast-menu-item.long").addClass("selected");jQuery("div.local-weather-forecast.selected").removeClass("selected");jQuery("div.local-weather-forecast.long").addClass("selected");jQuery('input[name="forecast"]').attr("value","long");jQuery("div.local-weather-forecast-metadata > span.probabilistic").css("display","inline-block")}jQuery(document).ready(function(){jQuery("li.local-weather-forecast-menu-item.long").css("display","inline-block");jQuery('input[name="forecast"]').attr("value","mid");jQuery("li.local-weather-forecast-menu-item.short > a").click(function(a){a.preventDefault();handle_forecast_short_click()});jQuery("li.local-weather-forecast-menu-item.mid > a").click(function(a){a.preventDefault();handle_forecast_mid_click()});jQuery("li.local-weather-forecast-menu-item.long > a").click(function(a){a.preventDefault();handle_forecast_long_click()})});/*]]>*/</script> <div class="short local-weather-forecast meteogram table-responsive"> <table class="meteogram"> <colgroup> <col class="first-day odd-day first-daily-step" /> <col class="first-day odd-day" /> <col class="first-day odd-day" /> <col class="first-day odd-day last-daily-step" /> <col class="last-day even-day first-daily-step" /> <col class="last-day even-day" /> <col class="last-day even-day" /> <col class="last